{
  "name": "TypeformResponses",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexbassy/TypeformResponses.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "start": "react-native start",
